    @{
        string userfullname = HttpContext.Current.User.Identity.Name;
        //string username = System.Web.HttpContext.Current.Request.LogonUserIdentity.Name;'

        try
        {          
            userfullname = Session["Userfullname"].ToString();
        }
        catch (Exception)
        {
            

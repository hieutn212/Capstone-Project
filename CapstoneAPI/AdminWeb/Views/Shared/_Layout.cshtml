    <script>
        $(document)
         .ajaxStart(function () {
             $('#loading-image').fadeIn(400);
         })
         .ajaxStop(function () {
             $('#loading-image').fadeOut(400, function () {
                 $(this).trigger('onFadeOutComplete');
             });
         });

